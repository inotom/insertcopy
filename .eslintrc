{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
    }
  },
  "rules": {
    "strict": 0,
    "semi": 2,
    "eqeqeq": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "no-trailing-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-console": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  }
}
